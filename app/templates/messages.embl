form submit="publishPost"
	= input .col-xs-12 value=message.title placeholder="Title"	
	= input .col-xs-12 value=message.body placeholder="Body"	
	button.col-xs-12 type="submit" send msg

section
	each message in arrangedContent
		div
			.subject.col-xs-12 = message.title
			.conetent.col-xs-12 = message.body
			link-to "message" this .col-xs-12| reply
